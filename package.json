{
  "name": "discord-controller",
  "version": "1.0.0",
  "description": "A wrapper around discord.js for easily creating discord bots with dependency injection",
  "main": "dist/index.js",
  "scripts": {
    "start": "nodemon --exec ts-node ./src/example/bot.ts",
    "build": "tsc",
    "pack": "mv ./build/src dist",
    "package": "npm run build && npm run pack",
    "clean": "rm -rf build && rm -rf dist",
    "test": "echo 'No tests created'",
    "lint": "npx @biomejs/biome lint ./src",
    "lint:fix": "npx @biomejs/biome lint --apply ./src",
    "format": "npx @biomejs/biome format ./src",
    "format:fix": "npx @biomejs/biome format --write ./src"
  },
  "keywords": [
    "bot",
    "discord"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Kylar101/discord-controller.git"
  },
  "author": "Kylar101",
  "license": "ISC",
  "dependencies": {
    "reflect-metadata": "^0.2.1"
  },
  "peerDependencies": {
    "discord.js": "^14.14.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.1",
    "@types/node": "^20.11.25",
    "@types/ws": "^8.5.10",
    "discord.js": "^14.14.1",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  }
}

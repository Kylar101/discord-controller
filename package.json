{
  "name": "discord-controller",
  "version": "0.9.3",
  "description": "A wrapper around discord.js for easily creating discord bots with dependency injection",
  "main": "dist/index.js",
  "scripts": {
    "start": "nodemon --exec ts-node ./src/example/bot.ts",
    "build": "tsc",
    "pack": "mv ./build/src dist",
    "package": "npm run build && npm run pack",
    "clean": "rm -rf build && rm -rf dist",
    "test": "echo 'No tests created'",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "bot",
    "discord"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Kylar101/discord-controller.git"
  },
  "author": "Kylar101",
  "license": "ISC",
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "peerDependencies": {
    "discord.js": "^12.2.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.14",
    "@types/ws": "^7.2.7",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "discord.js": "^12.2.0",
    "eslint": "^7.4.0",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  }
}

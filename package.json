{
  "name": "discord-controller",
  "version": "0.9.3",
  "description": "A wrapper around discord.js for easily creating discord bots with dependency injection",
  "main": "dist/index.js",
  "scripts": {
    "start": "nodemon --exec ts-node ./src/example/bot.ts",
    "build": "tsc",
    "pack": "mv ./build/src dist",
    "package": "npm run build && npm run pack",
    "clean": "rm -rf build && rm -rf dist",
    "test": "echo 'No tests created'",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "bot",
    "discord"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Kylar101/discord-controller.git"
  },
  "author": "Kylar101",
  "license": "ISC",
  "dependencies": {
    "reflect-metadata": "^0.2.1"
  },
  "peerDependencies": {
    "discord.js": "^14.14.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.25",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "discord.js": "^14.14.1",
    "eslint": "^8.57.0",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  }
}